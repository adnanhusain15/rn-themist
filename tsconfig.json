{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "declaration": true, /* Generates corresponding '.d.ts' file. */
        "jsx": "react",
        "lib": [
            "es6",
            "dom"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "target": "esnext",
        "types": [
            "react"
        ],
        "outDir": "dist"
    },
    "exclude": [
        "node_modules",
        "babel.config.js",
        "metro.config.js"
    ]
}